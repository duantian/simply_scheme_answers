
(define (roman-value letter)
  (cond ((equal? letter 'i) 1)
	((equal? letter 'v) 5)
	((equal? letter 'x) 10)
	((equal? letter 'l) 50)
	((equal? letter 'c) 100)
	((equal? letter 'd) 500)
	((equal? letter 'm) 1000)
	(else 0)))


(define (arabic s)
  (cond ((empty? s) 0)
	(else (+ (roman-value (first s)) (arabic (bf s))))))
